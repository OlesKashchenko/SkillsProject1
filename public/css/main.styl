@import 'nib'

/*******************
* Variables
*******************/
tdur = 400ms
transition = all tdur
delay(arguments)
  transition-delay: arguments

html, body
  height  100%

body
  margin 0 auto
  font-family 'GothamPro', Arial, Helvetica, sans-serif
  font-size 16px
  line-height 19px
  background #f9f9fa
  color #2c2c30
img
  border none
.wrapper
  position relative
  z-index 2
  min-width 1260px
  max-width 1600px
  width 100%
  min-height 600px
  height 100%
  margin: auto
.wrapper_inner
  min-height 100%
  position relative
  z-index 1
  background: #fff
.max_width
  position relative
  //max-width 1000px
  //margin 0 120px 0 60px
  width: 1100px
  margin: 0 auto

a
  color #2c2c30
  text-decoration none
  /*&:visited*/
  /*color #551a8b*/
  transition transition
  &:hover
    color #c42231
  &:focus
    outline thin dotted
  &:hover,
  &:active
    outline 0

h2
  margin 0
  font-size 24px
  line-height 28px
  font-weight bold
  color #2c2c30

pre
  white-space pre
  white-space pre-wrap
  word-wrap break-word

img
  border 0
  -ms-interpolation-mode bicubic
  vertical-align middle

@css {
  ::-moz-selection {
    background-color: #9c9c9e !important;
  }
  ::selection {
    background-color: #9c9c9e !important;
  }
}

/* =============================================================================
   Forms
   ========================================================================== */
form
  margin 0
fieldset
  border 0
  margin 0
  padding 0

label
  cursor pointer

legend
  border 0
  padding 0
  white-space normal

button,
input,
select,
textarea
  font-family 'GothamPro', Arial, Helvetica, sans-serif
  font-size 100%
  margin 0
  vertical-align baseline
  outline none !important
  &:focus
    border-color cornflowerblue
  .form .error &
    color red

button,
input
  line-height normal

button,
input[type="button"],
input[type="reset"],
input[type="submit"]
  cursor pointer
  -webkit-appearance button

button[disabled],
input[disabled]
  cursor default

input[type="checkbox"],
input[type="radio"]
  box-sizing border-box
  padding 0

input[type="search"]
  -webkit-appearance textfield
  -moz-box-sizing content-box
  -webkit-box-sizing content-box
  box-sizing content-box

button::-moz-focus-inner,
input::-moz-focus-inner
  border 0
  padding 0
textarea
  overflow auto
  vertical-align top
  resize vertical

/* Colors for form validity */
input:valid, textarea:valid {  }
input:invalid,
textarea:invalid
  background-color #f0dddd
  
.form
  .field
    position relative
    margin 0 0 20px
    &.error
      .error_txt
        display block
    &.field-focus
      input,
      textarea
        box-shadow 0 0 3px #dedede
      .input_helper_draggable
        .bar
          background #c42231
        a
          background-position -80px 0
  .input_holder
    position relative
    z-index 2
    padding 0 20px
    input,
    textarea
      display block
      width 100%
      padding 11px 17px
      border 1px solid #dedede
      border-radius 0
      margin 0 -20px
      background #ffffff
      font-size 14px
      line-height 28px
      letter-spacing .25px
      color #2a2a2a
      appearance none
      &:focus
        box-shadow: 0 0 3px #dedede
      &[type='checkbox']
        padding 0
        background none
    input
      height 28px
    textarea
      min-height 50px
  .select_holder
    z-index 1
  .placeholder
    absolute left top
    padding 12px 17px
    font-size 14px
    line-height 28px
    color #727272
    letter-spacing .25px
    cursor text
  label
    position relative
    span
      display inline-block
      margin-left 10px
      opacity 0.5
    &.custom_checkbox
      display inline-block
      padding-left 30px
      height 20px
      line-height 24px
      input
        position absolute
        z-index -1
        display none
        opacity 0
        visibility hidden
      &:before
        content " "
        absolute top left
        width 20px
        height 20px
        background url(../images/sprite.png) -300px 0 no-repeat
      &.checked:before
        background-position -280px 0
      &:hover:before
        box-shadow 0 0 5px rgba(0, 0, 0, .1)
    &.custom_switcher
      display: inline-block
      padding: 0
      input
        position absolute
        z-index -1
        display none
        opacity 0
        visibility hidden
      .switcher
        position: relative
        display: inline-block
        width 40px
        height 20px
        border-radius: 50%
        &:before
          content: " "
          absolute top left
          right: 0
          bottom: 0
          border: 1px solid #ababac
          border-radius: 10px
          transition transition
        &:after
          content: " "
          absolute top left
          width: 16px
          height: 16px
          margin: 2px
          border-radius: 50%
          background: #ababac
          transition transition
      &.checked
        .switcher
          &:after
            left 20px
      &:hover
        .switcher
          &:after
            background-color: #c42231
      &.white
        .switcher
          &:before
            border-color: #fff
          &:after
            background-color: #fff
        &:hover
          .switcher
            &:after
              background-color: #c42231
      span
        display inline-block
        vertical-align top
        margin: 0 10px 0 0
        font-size 15px
        line-height 20px
        opacity: 1
        cursor: pointer
  .error_txt
    display none
    position absolute
    top 100%
    left 0
    margin 0 5px 0 5px
    font-size 12px
    line-height 15px
    color red
  .input_helper_draggable
    absolute bottom left
    right 5px
    height 2px
    border none
    .bar
      content " "
      absolute top left
      height 100%
      background #bbbbc1
    a
      position absolute
      width 20px
      height 20px
      top 0
      margin -7px 0 0 -10px
      border none
      transition none
      outline none
      background url(../images/sprite.png) -60px 0 no-repeat
  .form_footer
    text-align center
  .form_infobar
    margin 50px 0 0
    height 60px
    clearfix()
    > div
      position relative
      float left
      height 100%
      color #ffffff
      .title
        absolute top 5px left 10px
        font-size 13px
      .descr
        absolute bottom 5px left 10px
        font-size 20px
      &.infobar_part_1
        width 40%
        background #c0d5ca
      &.infobar_part_2
        width 35%
        background #b1c9bd
      &.infobar_part_3
        width 25%
        background #ebe536
        color #2c2c30

/* =============================================================================
   Tables
   ========================================================================== */
table
  border-collapse collapse
  border-spacing 0
td
  vertical-align top
.dtable
  display table
.dtcell
  display table-cell
  vertical-align middle

/* =============================================================================
   Global elements
   ========================================================================== */
.inline-block
  displayinline-block
.none
  display none !important

.invisible
  visibility hidden
.hidden
  display none !important
  visibility hidden !important
.visible
  visibility visible !important

.clear
  clear both
  height 0
  font-size 0
  line-height 0
  overflow hidden
  display block
clearfix()
  zoom 1
  &:before,
  &:after
    content ""
    display table
  &:after
    clear both
.clearfix
  clearfix()

.fleft
  float left !important
.fright
  float right !important

.vat
  vertical-align top !important
.vam
  vertical-align middle !important
.vab
  vertical-align bottom !important

.tac
  text-align center !important
.tar
  text-align right !important

.not_select
  user-select none

/* =============================================================================
   Icon
   ========================================================================== */
.icon
  display inline-block
  width 20px
  height 20px
  background-image url(../images/sprite.png)
  background-repeat no-repeat
  &.icon_sitemap
    width 40px
    height 40px
    background-position -280px -20px
  &.icon_search
    width 40px
    height 40px
    background-position -320px -20px
  &.icon_fb
    width: 40px
    height: 40px
    background-position -120px -300px
  &.icon_vk
    width: 40px
    height: 40px
    background-position -160px -300px
  &.icon_od
    width: 40px
    height: 40px
    background-position -200px -300px
  &.icon_tw
    width: 40px
    height: 40px
    background-position -240px -300px
  &.icon_li
    width: 40px
    height: 40px
    background-position -120px -340px
  &.icon_yt
    width: 40px
    height: 40px
    background-position -160px -340px
  &.icon_gp
    width: 40px
    height: 40px
    background-position -200px -340px
  &.icon_rs
    width: 40px
    height: 40px
    background-position -240px -340px
  &.icon_apple
    width 40px
    height 40px
    background-position -100px -140px
  &.icon_gplay
    width 40px
    height 40px
    background-position -140px -140px
  &.icon_phone
    width 40px
    height 40px
    background-position -300px -260px

.icon_close
  position relative
  transform rotate(-45deg)
  background none
  font-family Tahoma, Geneva, sans-serif
  font-size 36px
  line-height 36px
  color #313131
  font-style normal
  transition transition
  &:hover
    color #5e5e5e

/* =============================================================================
   Btn
   ========================================================================== */
.btn
  position relative
  z-index 1
  display inline-block
  height 20px
  padding 10px 25px
  font-size 13px
  line-height 23px
  font-weight bold
  text-transform uppercase
  text-align center
  color transparent
  background #ffffff
  border none
  border-radius 3px
  cursor pointer
  overflow hidden
  transition transition
  a, span
    absolute top left
    z-index 1
    width 100%
    height 100%
    display block
    font-size 13px
    line-height 23px
    font-weight bold
    text-transform uppercase
    text-align center
    color transparent
    opacity 0
  &:before
    display: none
    content attr(data-text)
    absolute bottom 120% left
    width 100%
    height 100%
    line-height 43px
    letter-spacing: 0.5px
    color #2c2c30
    transition transition
  &:after
    content attr(data-text)
    position relative
    display block
    width 100%
    height 100%
    line-height 23px
    letter-spacing: 0.5px
    color #2c2c30
    transition transition
  &:hover
    //line-height 150px
    background: #f2f2f2
    //&:before
    //  bottom 0
    //&:after
    //  line-height 150px

  &.btn_big
    padding 15px 50px
    font-size: 16px
    &:before
      line-height 53px

  &.btn_red
    background-color #c42231
    &:before
    &:after
      color #ffffff
    &:hover
      background: #b01f2c
  &.btn_black
    background-color #3a3a3e
    &:before
    &:after
      color #ffffff
    &:hover
      background: #28282b
  &.btn_yellow
    background-color #ebe536
    &:hover
      background: #dfd933
  &.btn_transparent
    background: transparent
    border: 2px solid #fff
    border-color: rgba(255, 255, 255, .2)
    &:before
    &:after
      color: #fff
    &:hover
      background: transparent
      border-color: rgba(255, 255, 255, 1)

.input_btn
  absolute top left
  z-index 1
  width 100%
  height 100%
  padding 0
  font-size 13px
  line-height 20px
  font-weight bold
  text-transform uppercase
  text-align center
  background none
  color #2c2c30
  border none
  opacity 0

/* =============================================================================
