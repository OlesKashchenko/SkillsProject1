/*info_window*/
.info_window
  position absolute
  left 0
  top 0
  margin-top -75px
  .iw_inner
    position absolute
    left 35px
    top 0
    z-index 1
    padding 30px 30px 30px 110px
    //width 260px
    min-height 70px
    font-size 14px
    line-height 21px
    text-align left
    font-style normal
    font-weight 300
    text-transform none
    color #929292
    background white
    border-radius 5px
    &:before
      content: " "
      position: absolute
      top 0
      left 0
      width: 80px
      height: 100%
      background: #f7f8f9
      border-top-left-radius 5px
      border-bottom-left-radius 5px
    &:after
      content ' '
      position absolute
      right 100%
      top 45px
      border-width 10px 10px 10px 0
      border-style solid
      border-color transparent #f7f8f9
      margin -10px 0
  .iw_close
    position absolute
    top 5px
    right 5px
    width 20px
    height 20px
    background url("../images/sprite.png") no-repeat -40px 0
    cursor pointer
    opacity .25
    transition all 300ms
    &:hover
      opacity: 1
  .iw_path
    position absolute
    top 25px
    left 20px
    width 40px
    height 20px
    background url("../images/sprite.png") no-repeat -360px -60px
    cursor pointer
    opacity .25
    transition all 300ms
    &:hover
      opacity: 1
  .iw_print
    position absolute
    top 80px
    left 30px
    width 20px
    height 20px
    background url("../images/sprite.png") no-repeat -20px 0
    cursor pointer
    opacity .25
    transition all 300ms
    &:hover
      opacity: 1
  h4
    margin -5px 0 0
    color black
    text-transform uppercase
    font-weight normal
    letter-spacing .5px
  .hr
    width 130px
    margin 22px 0
  .fuels
    color #404040
    letter-spacing .25px
    margin 22px 0
  .services
    margin-left -10px
